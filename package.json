{
  "name": "react-redux-ts-boilerplate",
  "version": "0.0.1",
  "description": "React Redux TypeScript boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "NODE_ENV=production webpack",
    "compile:dev": "NODE_ENV=development webpack",
    "compile:server": "NODE_ENV=production webpack --config=webpack.server.config.js",
    "start": "webpack-dev-server --hot",
    "start:server": "node dist/server/server.js",
    "test": "NODE_ENV=test ./node_modules/.bin/karma start --single-run",
    "test:dev": "NODE_ENV=test ./node_modules/.bin/karma start"
  },
  "author": "Ludek Stepan (https://github.com/ludekstepan)",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "@types/react": "^15.0.24",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "babel-core": "^6.21.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "html-webpack-plugin": "^2.24.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.13",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.1",
    "webpack": "2.5.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-visualizer-plugin": "^0.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ludekstepan/ts-boilerplate.git"
  }
}
